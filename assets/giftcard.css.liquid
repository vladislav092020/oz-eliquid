    {%- endif -%}

    {%- if font_style_name == 'italic' -%}
      {%- assign font_style = 'italic' -%}
    {%- endif -%}

    {%- assign font_file = font_split[1] | strip -%}
  {%- endif -%}

  {%- unless font_weight_name == 'initial' -%}
