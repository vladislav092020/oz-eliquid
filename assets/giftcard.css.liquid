          {%- assign font_weight = font_weight_json -%}
        {%- endif -%}
      {%- endif -%}
    {%- elsif font_weight_name contains 'bolder' -%}
      {%- if font_weight_name == 'bolder' -%}
        {%- assign font_weight = 700 -%}
